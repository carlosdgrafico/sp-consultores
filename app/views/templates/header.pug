header.header
	.row.u-resetRow
		.col.s2
			figure.logotipo.header__logotipo
				img.logotipo__img(src="./img/logotipo-aCambio-white.png", alt="Logotipo aCambio")

		.col.s10.u-right
			ul.header__menu.header__menu--mobile
				li.header__item
					a.header__link(href="#que-hacemos") ¿Qué hacemos?
				li.header__item
					a.header__link(href="#como-funciona") ¿Cómo funciona?
				li.header__item
					a.header__link.header__link--active(href="#quienes-somos") ¿Quiénes somos?
				li.header__item
					a.header__link(href="#quienes-pueden") Colaboradores
				li.header__item
					a.header__link(href="javascript:;") Blog
				li.header__item
					a.header__link(href="#contacto") Contacto

			a.header__usuario(href="javascript:;")
				span.header__usuario-border
				figure.header__usuario-foto
					img(src="./img/user.jpg", alt="Fotografica del usuario que ha iniciado sesión")


			.burger
				.burger__container
					.burger__stripe.burger__stripe--top
					.burger__stripe.burger__stripe--middle
					.burger__stripe.burger__stripe--bottom

